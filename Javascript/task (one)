/******* Start Your Code *********/

function oddArray(){
    var nums = [];
    for (var i = 1; i<= 9; i++) {
        if ((i % 2) == 1) {
          nums.push(i)
         
        }
    }
   
    return nums;
   
  }
  console.log(oddArray());
  console.log('=====================');
  
  
  var i = 1;
  while (i < 10)  { 
    console.log( i );
    i+=2;
    
  }
  oddArray()
console.log('=====================');


